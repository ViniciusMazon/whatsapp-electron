{
  "name": "whatsapp-electron",
  "version": "1.0.0",
  "description": "A simple whatsapp app in electron",
  "main": "./src/background.js",
  "scripts": {
    "start": "electron ./src/background.js",
    "pack": "electron-builder ./src/background.js",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.mazon.whatsapp",
    "linux": {
      "category": "social",
      "icon": "build/icon.png",
      "target": ["deb"]
    }
  },
  "keywords": [],
  "author": "ViniciusMazon <vnpmazon@gmail.com> (http://github.com/viniciusmazon)",
  "homepage": "https://www.github.com/ViniciusMazon/whatsapp-electron",
  "license": "MIT",
  "devDependencies": {
    "electron": "^19.0.7",
    "electron-builder": "^23.1.0"
  }
}
